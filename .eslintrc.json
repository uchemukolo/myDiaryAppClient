{
    "extends": "airbnb",
    "rules": {
        "semi": ["error", "always"],
        "one-var": 0,
        "one-var-declaration-per-line": 0,
        "new-cap": 0,
        "max-len": [1, 80, 2],
        "consistent-return": 0,
        "no-param-reassign": 0,
        "no-useless-escape": 0,
        "no-case-declarations": 0,
        "comma-dangle": 0,
        "curly": ["error", "multi-line"],
        "import/no-unresolved": [
          2
        ]
    }
}